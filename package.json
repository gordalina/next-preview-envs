{
  "name": "next-preview-envs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx dotenv -e .env.local -- next dev",
    "build": "next build",
    "start": "npx dotenv -e .env.local -- next start",
    "lint": "next lint",
    "db:reset": "npx dotenv -e .env.local -- prisma migrate reset --force && npm run db:push && npm run db:seed",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "db:seed": "npx dotenv -e .env.local -- node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.6.0"
  }
}
